{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/test.html"
    },
    {
      "src": "/(.*\\.(js|css|map))$",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/test.html"
    }
  ]
}